{
   "version": 2,
   "name": "lenguapedia-wikia",
   "builds": [
      { "src": "index.mjs", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "/index.mjs" }
   ],
    "headers": [
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "no-cache"
        },
            {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-cache"
        },
            {
          "key": "CDN-Cache-Control",
          "value": "no-cache"
        },
            {
          "key": "Cache-Control",
          "value": "no-cache"
        },
            {
          "key": "Surrogate-Control",
          "value": "no-cache"
        }
      ]
}